#include<stdio.h>
void main
{
    int i,num,sum=0;
    printf("enter an integer");
    scanf("%d",&num);
    for(i=1;i<=num;i++)
    {
        sum=sum+i;
    }
printf("sum of first %d natural numbers=%d",num,sum");
    
out

3
